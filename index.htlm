<body>
    
    <div class="readonly-badge">üëÅÔ∏è Lecture seule</div>
    <div class="readonly-info">
        üìñ Mode consultation - Aucune modification possible
    </div>

    <div id="map"></div>
    
    <div class="loading-indicator" id="loading-indicator">
        <div id="loading-text">‚è≥ Chargement...</div>
        <div style="font-size: 12px; margin-top: 5px;" id="loading-subtext"></div>
    </div>
    
    <div class="longpress-indicator" id="longpress-indicator">
        üìç Maintenez pour ajouter un coffret...
    </div>
    
    <div class="search-panel expanded" id="search-panel">
        <div class="search-panel-header" onclick="toggleSearchPanel()">
            <h3>üîç Rechercher <span class="hybrid-badge">HYBRIDE</span></h3>
            <button class="toggle-btn">‚óÄ</button>
        </div>
        <div class="search-panel-content">
            <input type="text" 
                   class="search-input" 
                   id="search-input" 
                   placeholder="Rechercher par installation, implantation ou d√©signation..."
                   onkeyup="performSearch()">
            <div class="filter-tags" id="filter-tags"></div>
            <div class="search-results" id="search-results">
                <div class="no-results">Tapez pour rechercher...</div>
            </div>
        </div>
    </div>
    
    <div class="info-panel collapsed" id="info-panel">
        <div class="info-panel-header" onclick="toggleInfoPanel()">
            <h3>‚ö° Coffrets</h3>
            <button class="toggle-btn">‚ñ∂</button>
        </div>
        <div class="info-panel-content">
            <p><strong>Total:</strong> <span id="total-number">312</span> coffrets</p>
            <p><strong>Avec photos:</strong> <span id="photo-number">0</span></p>
            <p><strong>Avec PDF:</strong> <span id="pdf-number">0</span></p>
            <p style="font-size: 11px; color: #666;">üìç Appui long pour ajouter</p>
            <div class="storage-info">
                <div style="font-weight: bold; margin-bottom: 5px;">üíæ Stockage hybride:</div>
                <div class="storage-bar">
                    <div class="storage-bar-fill" id="storage-bar" style="width: 0%"></div>
                </div>
                <div style="font-size: 10px; margin-top: 3px;">
                    <span id="storage-text">0 / 1000 miniatures</span>
                </div>
                <div style="font-size: 10px; color: #666; margin-top: 5px;">
                    üñºÔ∏è Mini (5KB) + üîó HD (Drive)
                </div>
            </div>
        </div>
    </div>
    
    <div class="export-panel">
        <button class="btn-export" onclick="exportData()">üíæ Exporter</button>
        <input type="file" id="import-file" accept=".json" style="display:none" onchange="importData(event)">
        <!-- Import d√©sactiv√© en lecture seule -->
        <!-- Stockage d√©sactiv√© en lecture seule -->
    </div>
    
    <div id="add-marker-modal" class="add-marker-modal">
        <div class="modal-content">
            <div class="modal-header">
                <h2>‚ûï Nouveau coffret</h2>
                <button class="modal-close" onclick="closeAddMarkerModal()">&times;</button>
            </div>
            <form id="add-marker-form">
                <div class="form-group">
                    <label>üìç Coordonn√©es GPS</label>
                    <div class="coords-display" id="new-coords"></div>
                </div>
                <div class="form-group">
                    <label>üîå D√©signation *</label>
                    <input type="text" id="new-designation" required placeholder="Ex: TGBT 400V">
                </div>
                <div class="form-group">
                    <label>üè∑Ô∏è MPSCA</label>
                    <input type="text" id="new-mpsca" placeholder="Ex: MPSCA-001">
                </div>
                <div class="form-group">
