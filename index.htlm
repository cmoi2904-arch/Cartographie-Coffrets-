            const totalPDFs = markersData.reduce((count, m) => {
                return count + (m.pdfLink ? 1 : 0);
            }, 0);
            
            const percentage = (totalPhotos / 1000) * 100;
            const barFill = document.getElementById('storage-bar');
            
            barFill.style.width = percentage + '%';
            
            if (percentage > 90) {
                barFill.className = 'storage-bar-fill danger';
            } else if (percentage > 70) {
                barFill.className = 'storage-bar-fill warning';
            } else {
                barFill.className = 'storage-bar-fill';
            }
            
            document.getElementById('storage-text').textContent = `${totalPhotos} / 1000 miniatures`;
            document.getElementById('pdf-number').textContent = totalPDFs;
        }
        
        // Fonctions de gestion du stockage
        function getStorageInfo() {
            let totalSize = 0;
            for (let key in localStorage) {
                if (localStorage.hasOwnProperty(key)) {
                    totalSize += (localStorage[key].length + key.length) * 2;
                }
            }
            
            const totalPhotos = markersData.reduce((count, m) => {
                return count + (m.photos ? m.photos.filter(p => p.thumbnail !== null).length : 0);
            }, 0);
            
            return {
                sizeBytes: totalSize,
                sizeMB: (totalSize / (1024 * 1024)).toFixed(2),
                photos: totalPhotos,
                percentage: ((totalSize / (5 * 1024 * 1024)) * 100).toFixed(1)
            };
        }
        
