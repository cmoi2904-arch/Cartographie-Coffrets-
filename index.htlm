        window.addPhoto = async function(markerId, photoIndex, event) {
            const input = event.target;
            let file = input.files[0];
            
            if (file) {
                if (file.size > 10 * 1024 * 1024) {
                    alert('‚ùå La photo est trop volumineuse (max 10MB avant compression)');
                    input.value = '';
                    return;
                }
                
                try {
                    document.getElementById('loading-indicator').classList.add('active');
                    document.getElementById('loading-text').textContent = '‚è≥ Traitement de la photo...';
                    document.getElementById('loading-subtext').textContent = 'Conversion et compression en cours';
                    
                    // Convertir HEIC en JPEG si n√©cessaire
                    file = await convertHeicToJpeg(file);
                    
                    document.getElementById('loading-text').textContent = '‚è≥ Compression miniature...';
                    document.getElementById('loading-subtext').textContent = 'Optimisation ultra-compress√©e (5KB)';
                    
                    const compressedData = await compressImageMiniature(file, 600, 0.4);
                    
                    const marker = markersData.find(m => m.id === markerId);
                    if (!marker) {
                        throw new Error('Marqueur non trouv√©');
                    }
                    
                    marker.photos[photoIndex].thumbnail = compressedData;
                    marker.photos[photoIndex].hdLink = null; // L'utilisateur pourra ajouter le lien HD apr√®s
                    saveAllData();
                    
                    const hasPhotos = marker.photos.some(p => p.thumbnail !== null);
                    if (hasPhotos) {
                        markers[markerId].setIcon(electricIconWithPhoto);
                    }
                    
                    markers[markerId].closePopup();
                    setTimeout(() => {
                        markers[markerId].setPopupContent(createPopupContent(marker));
                        markers[markerId].openPopup();
                        document.getElementById('loading-indicator').classList.remove('active');
                        
                        // Proposer d'ajouter le lien HD
                        if (confirm('üì∑ Miniature ajout√©e!\n\nVoulez-vous ajouter le lien vers la photo HD (4MB) sur Google Drive?')) {
                            editPhotoHDLink(markerId, photoIndex);
                        }
                    }, 100);
                    
                } catch (error) {
                    console.error('Erreur:', error);
                    if (error.name === 'QuotaExceededError') {
                        alert('‚ùå STOCKAGE SATUR√â!\n\nüíæ Exportez vos donn√©es\nüóëÔ∏è Supprimez des photos anciennes');
                    } else {
                        alert('‚ùå Erreur: ' + error.message);
                    }
                    document.getElementById('loading-indicator').classList.remove('active');
                }
                
                input.value = '';
            }
        };
        
        window.removePhoto = function(markerId, photoIndex) {
            if (confirm('Voulez-vous vraiment supprimer cette photo (miniature + lien HD)?')) {
                const marker = markersData.find(m => m.id === markerId);
                marker.photos[photoIndex] = {thumbnail: null, hdLink: null};
                saveAllData();
                
                const hasPhotos = marker.photos.some(p => p.thumbnail !== null);
                if (!hasPhotos) {
                    markers[markerId].setIcon(electricIcon);
                }
                
                markers[markerId].closePopup();
                setTimeout(() => {
                    markers[markerId].setPopupContent(createPopupContent(marker));
                    markers[markerId].openPopup();
                }, 100);
            }
        };
        
        function addMarkerToMap(marker) {
            const hasPhotos = marker.photos && marker.photos.some(p => p.thumbnail !== null);
            const icon = hasPhotos ? electricIconWithPhoto : electricIcon;
            
            const leafletMarker = L.marker([marker.lat, marker.lon], {icon: icon})
                .bindPopup(createPopupContent(marker), {
                    maxWidth: 360,
                    minWidth: 256
                })
                .addTo(map);
            
            markers[marker.id] = leafletMarker;
        }
        
        map = L.map('map').setView([43.1980155, 5.349513535576923], 18);
        
        const satelliteLayer = L.tileLayer('https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}', {
            attribution: 'Tiles ¬© Esri',
            maxZoom: 22
        });
        
        const osmLayer = L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: '¬© OpenStreetMap contributors',
            maxZoom: 22
        });
        
        const topoLayer = L.tileLayer('https://{s}.tile.opentopomap.org/{z}/{x}/{y}.png', {
            attribution: 'Map data: ¬© OpenStreetMap contributors',
            maxZoom: 22
        });
        
        satelliteLayer.addTo(map);
        
        const baseLayers = {
            "Satellite": satelliteLayer,
            "Plan": osmLayer,
            "Topographique": topoLayer
        };
        
        L.control.layers(baseLayers).addTo(map);
        
        const electricIcon = L.icon({
            iconUrl: 'data:image/png;base64,',
            iconSize: [32, 32],
            iconAnchor: [16, 32],
            popupAnchor: [0, -32]
        });
        
        const electricIconWithPhoto = L.icon({
            iconUrl: 'data:image/png;base64,',
            iconSize: [32, 32],
            iconAnchor: [16, 32],
            popupAnchor: [0, -32],
            className: 'marker-with-photo'
        });
        
        // Appui long d√©sactiv√© en mode lecture seule
        /*map.on('mousedown', function(e) {
            const indicator = document.getElementById('longpress-indicator');
            indicator.style.left = e.originalEvent.clientX + 10 + 'px';
            indicator.style.top = e.originalEvent.clientY + 10 + 'px';
            indicator.style.display = 'block';
            
            longPressTimer = setTimeout(() => {
                indicator.style.display = 'none';
                openAddMarkerModal(e.latlng.lat, e.latlng.lng);
            }, 1000);
        });
        
        map.on('mouseup', function() {
            if (longPressTimer) {
                clearTimeout(longPressTimer);
                longPressTimer = null;
            }
            document.getElementById('longpress-indicator').style.display = 'none';
        });
        
        map.on('mousemove', function() {
            if (longPressTimer) {
                clearTimeout(longPressTimer);
                longPressTimer = null;
                document.getElementById('longpress-indicator').style.display = 'none';
            }
        });
        
        */ // Fin d√©sactivation appui long
        /*map.on('contextmenu', function(e) {
            e.originalEvent.preventDefault();
            openAddMarkerModal(e.latlng.lat, e.latlng.lng);
        });*/
        
        loadPhotos();
        restorePanelStates();
        
        const installations = [...new Set(markersData.map(m => m.installation))].sort();
        const filterTagsContainer = document.getElementById('filter-tags');
        installations.slice(0, 5).forEach(installation => {
            const tag = document.createElement('span');
            tag.className = 'filter-tag';
            tag.textContent = installation;
            tag.onclick = () => {
                document.getElementById('search-input').value = installation;
                performSearch();
            };
            filterTagsContainer.appendChild(tag);
        });
        
        markersData.forEach(marker => {
            addMarkerToMap(marker);
        });
        
        const bounds = L.latLngBounds(markersData.map(m => [m.lat, m.lon]));
        map.fitBounds(bounds, {padding: [50, 50]});
    </script>
</body>
</html>