                    [convertedBlob], 
                    file.name.replace(/\.heic$/i, '.jpg').replace(/\.heif$/i, '.jpg'),
                    { type: 'image/jpeg' }
                );
                
                console.log('Conversion HEIC termin√©e');
                return convertedFile;
                
            } catch (error) {
                console.error('Erreur conversion HEIC:', error);
                throw new Error('Impossible de convertir le fichier HEIC. Essayez de le convertir en JPG manuellement.');
            }
        }
        
        // Compression ultra-optimis√©e pour miniatures (5 KB)
        function compressImageMiniature(file, maxWidth = 600, quality = 0.4) {
            return new Promise((resolve, reject) => {
                const reader = new FileReader();
                reader.onload = (e) => {
                    const img = new Image();
                    img.onload = () => {
                        const canvas = document.createElement('canvas');
                        let width = img.width;
                        let height = img.height;
                        
                        if (width > maxWidth) {
                            height = (height * maxWidth) / width;
                            width = maxWidth;
                        }
                        
                        canvas.width = width;
                        canvas.height = height;
                        
                        const ctx = canvas.getContext('2d');
                        ctx.drawImage(img, 0, 0, width, height);
                        
                        const compressedData = canvas.toDataURL('image/jpeg', quality);
                        resolve(compressedData);
                    };
                    img.onerror = reject;
                    img.src = e.target.result;
                };
                reader.onerror = reject;
                reader.readAsDataURL(file);
            });
        }
        
        function updateStorageIndicator() {
            const totalPhotos = markersData.reduce((count, m) => {
